{
  "name": "tutorial-with-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "deno task dev:api & deno task dev:vite",
    "dev:api": "deno run --allow-env --allow-read --allow-net api/main.ts",
    "dev:vite": "deno run -A npm:vite",
    "build": "tsc -b && vite build",
    "serve": "deno task build && deno task dev:api",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "push": "rsync -avz --progress ./ saltchicken@10.0.0.9:/home/saltchicken/server/",
    "deploy": "deno task push && ssh 10.0.0.9 'cd /home/saltchicken/server/ && /home/saltchicken/.deno/bin/deno task dev --host'"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
